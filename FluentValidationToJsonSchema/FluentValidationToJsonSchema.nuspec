<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>FluentValidationToJsonSchema</id>
    <version>1.0.0</version>
    <authors>zabaweb</authors>
    <owners>zabaweb</owners>
    <description>A library to convert FluentValidation rules to JSON Schema.</description>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>https://github.com/YourRepo/FluentValidationToJsonSchema</projectUrl>
    <tags>FluentValidation JSON Schema</tags>
    <dependencies>
      <dependency id="FluentValidation" version="10.3.6" />
      <dependency id="Newtonsoft.Json" version="13.0.3" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin/Release/net6.0/*.dll" target="lib/net6.0" />
    <file src="bin/Release/netstandard2.0/*.dll" target="lib/netstandard2.0" />
  </files>
</package>
